<p id="notice"><%= notice %></p>

<h1>Products</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Manufacturer</th>
      <th>Release year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% cache @products do %>
      <%= render 'products', locals: { products: @products } %>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product', new_product_path %>
